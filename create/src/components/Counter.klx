<template>
  <button @click="increment">
    Count is: {{ count }}
  </button>
</template>

<script>
import { ref } from '@kalxjs-framework/runtime'

export default {
  name: 'Counter',
  setup() {
    const count = ref(0)
    const increment = () => count.value++
    return { count, increment }
  }
}
</script>