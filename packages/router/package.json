{
  "name": "@kalxjs/router",
  "version": "2.0.1",
  "description": "Advanced router package for KalxJs framework with features that surpass Vue Router",
  "type": "module",
  "author": "kalxjs Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Odeneho-Calculus/kalxjs.git"
  },
  "bugs": {
    "url": "https://github.com/Odeneho-Calculus/kalxjs/issues"
  },
  "homepage": "https://github.com/Odeneho-Calculus/kalxjs#readme",
  "keywords": [
    "kalxjs",
    "router",
    "spa",
    "single-page-application",
    "navigation",
    "history",
    "vue-router-alternative",
    "advanced-routing"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist",
    "src",
    "docs"
  ],
  "scripts": {
    "build": "rollup -c ../../rollup.config.js",
    "dev": "rollup -c ../../rollup.config.js -w",
    "test": "jest",
    "docs": "typedoc --out docs/api src/index.js",
    "prepublishOnly": "npm run build && npm run docs"
  },
  "dependencies": {
    "@kalxjs/core": "^2.1.17"
  },
  "peerDependencies": {
    "@kalxjs/core": "^2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "gitHead": "fbf879495879979f9e165e77328a0293b37a7f93"
}
