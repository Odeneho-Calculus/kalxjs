{
  "name": "@kalxjs/devtools",
  "version": "1.2.11",
  "description": "Developer Tools for KalxJs framework",
  "type": "module",
  "author": "kalxjs Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Odeneho-Calculus/kalxjs.git"
  },
  "bugs": {
    "url": "https://github.com/Odeneho-Calculus/kalxjs/issues"
  },
  "homepage": "https://github.com/Odeneho-Calculus/kalxjs#readme",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "dev": "rollup -c rollup.config.js -w",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/rollup-win32-x64-msvc": "^4.40.1",
    "rollup": "^4.40.1"
  }
}
