{
  "name": "@kalxjs/core",
  "version": "1.2.7",
  "description": "A modern JavaScript framework for building user interfaces",
  "type": "module",
  "author": "kalxjs Team",
  "license": "MIT",
  "keywords": [
    "kalxjs",
    "framework",
    "javascript",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Odeneho-Calculus/kalxjs.git"
  },
  "bugs": {
    "url": "https://github.com/Odeneho-Calculus/kalxjs/issues"
  },
  "homepage": "https://github.com/Odeneho-Calculus/kalxjs#readme",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c ../../rollup.config.js",
    "dev": "rollup -c ../../rollup.config.js -w",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "optionalDependencies": {
    "@kalxjs/cli": "^1.2.28",
    "@kalxjs/devtools": "^1.2.8",
    "@kalxjs/router": "^1.2.7",
    "@kalxjs/state": "^1.2.7"
  },
  "suggests": {
    "@kalxjs/router": "^1.2.6",
    "@kalxjs/state": "^1.2.6",
    "@kalxjs/devtools": "^1.2.7",
    "@kalxjs/cli": "^1.2.27"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
