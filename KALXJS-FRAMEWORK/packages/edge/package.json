{
    "name": "@kalxjs/edge",
    "version": "1.0.1",
    "description": "Edge Computing support for KalxJS - Cloudflare Workers, Deno Deploy, and edge runtime optimization",
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "types": "types/index.d.ts",
    "files": [
        "dist",
        "src",
        "types",
        "README.md"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "require": "./dist/index.cjs.js",
            "types": "./types/index.d.ts"
        },
        "./cloudflare": {
            "import": "./dist/cloudflare.esm.js",
            "require": "./dist/cloudflare.cjs.js"
        },
        "./deno": {
            "import": "./dist/deno.esm.js"
        },
        "./vercel": {
            "import": "./dist/vercel.esm.js",
            "require": "./dist/vercel.cjs.js"
        }
    },
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "jest"
    },
    "keywords": [
        "kalxjs",
        "edge",
        "cloudflare-workers",
        "deno-deploy",
        "vercel-edge",
        "edge-computing",
        "serverless",
        "ssr"
    ],
    "author": "KalxJS Team",
    "license": "MIT",
    "dependencies": {
        "@kalxjs/core": "^3.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "jest": "^29.5.0",
        "rollup": "^2.79.1"
    }
}
