{
  "name": "@kalxjs/native",
  "version": "2.0.2",
  "description": "Native Mobile and Desktop support for KalxJS - React Native-like API with Electron and Tauri integration",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "src",
    "types",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./types/index.d.ts"
    },
    "./electron": {
      "import": "./dist/electron.esm.js",
      "require": "./dist/electron.cjs.js"
    },
    "./tauri": {
      "import": "./dist/tauri.esm.js",
      "require": "./dist/tauri.cjs.js"
    },
    "./mobile": {
      "import": "./dist/mobile.esm.js",
      "require": "./dist/mobile.cjs.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest"
  },
  "keywords": [
    "kalxjs",
    "native",
    "mobile",
    "desktop",
    "electron",
    "tauri",
    "react-native",
    "cross-platform"
  ],
  "author": "KalxJS Team",
  "license": "MIT",
  "dependencies": {
    "@kalxjs/core": "^3.0.0"
  },
  "peerDependencies": {
    "@tauri-apps/api": "^1.5.0",
    "electron": "^28.0.0"
  },
  "peerDependenciesMeta": {
    "electron": {
      "optional": true
    },
    "@tauri-apps/api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "jest": "^29.5.0",
    "rollup": "^2.79.1"
  },
  "gitHead": "13e16c8e24d110351a3a2fb5314d5904ae35784e"
}
